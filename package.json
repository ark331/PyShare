{
  "name": "pyshare",
  "version": "1.0.0",
  "description": "Share and receive files over local network",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "android:sync": "npx cap sync android",
    "android:open": "npx cap open android",
    "android:build:debug": "npm run android:sync && cd android && ./gradlew assembleDebug",
    "android:build:release": "npm run android:sync && cd android && ./gradlew assembleRelease",
    "android:clean": "cd android && ./gradlew clean",
    "android:install": "cd android && ./gradlew installDebug"
  },
  "keywords": [
    "file-sharing",
    "network",
    "http-server"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@capacitor/android": "^8.0.0",
    "@capacitor/browser": "^8.0.0",
    "@capacitor/cli": "^8.0.0",
    "@capacitor/core": "^8.0.0",
    "@capacitor/filesystem": "^8.0.0",
    "archiver": "^6.0.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "nodejs-mobile-cordova": "^0.4.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
